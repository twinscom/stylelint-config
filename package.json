{
  "name": "@twinscom/stylelint-config",
  "version": "6.0.1",
  "description": "Stylelint Shareable Config",
  "main": ".stylelintrc.json",
  "scripts": {
    "test": "stylelint **/*.css && stylelint --config ./scss.json **/*.scss",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twinscom/stylelint-config.git"
  },
  "keywords": [
    "stylelint",
    "config",
    "stylelint-config"
  ],
  "author": {
    "name": "Evgeny Orekhov",
    "url": "https://plus.google.com/+ЕвгенийОрехов"
  },
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/twinscom/stylelint-config/issues"
  },
  "homepage": "https://github.com/twinscom/stylelint-config#readme",
  "files": [
    "base.json",
    ".stylelintrc.json",
    "scss.json"
  ],
  "peerDependencies": {
    "stylelint": ">=14.1.0"
  },
  "dependencies": {
    "prettier": "^2.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-group-selectors": "^1.0.8",
    "stylelint-prettier": "^2.0.0"
  },
  "devDependencies": {
    "stylelint": "^14.1.0"
  }
}
